<div id="created_events" class="admin_block">
<a id="created"></a>
  <div class="wrapper admin-event-list admin-created">
    <h2>groupacts you've created:</h2>
      <ul>
        <% user.created_events.each do |event| %>
          <li>
            <%= link_to (image_tag event.image, :size => "287x137"), event_path(event.url) %>
            <span class="event_image_title">
              <%= link_to event.title, event_path(event.url) %>
            </span>
          </li>
        <% end %>
      </ul>
    <p class="user_admin_nav"><a href="#top">top</a></p>
  </div>
</div>